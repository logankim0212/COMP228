/** Exercise01: Insurance.java
 * Author: Logan Kim
 * Purpose: Superclass of Health and Life classes
*/
package application;

public abstract class Insurance {
	// field variables
	protected String _insuranceType;
	protected double _monthlyCost;

	// getter
	public String getInsuranceType() {
		return _insuranceType;
	}
	public double getMonthlyCost() {
		return _monthlyCost;
	}

	// abstract methods
	public abstract void setInsuranceCost(double monthlyCost);
	public abstract void displayInfo();
} // end class

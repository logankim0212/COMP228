package application;

public class Health extends Insurance {
	public Health() {
		_insuranceType = "Health";
	}

	@Override
	public void setInsuranceCost(double monthlyCost) {
		_monthlyCost = monthlyCost;
	}
	public void displayInfo() {
		System.out.println("This " + _insuranceType + " insurance costs $" + _monthlyCost);
	}
}

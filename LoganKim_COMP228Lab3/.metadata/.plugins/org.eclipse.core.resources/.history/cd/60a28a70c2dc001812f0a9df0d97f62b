package application;

public class Health extends Insurance {
	public Health() {
		_insuranceType = "Health";
	}

	@Override
	public void setInsuranceCost(double monthlyCost) {
		_monthlyCost = monthlyCost;
	}
	public void displayInfo(int i) {
		System.out.println("Insurance " + i + ": Monthly fee of " + _insuranceType + " insurance is $" + _monthlyCost);
	}
}

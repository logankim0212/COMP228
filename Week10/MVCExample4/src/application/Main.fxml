<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <Label layoutX="108.0" layoutY="6.0" prefHeight="68.0" prefWidth="184.0" text="Example4">
         <font>
            <Font name="System Bold" size="31.0" />
         </font>
      </Label>
      <Button fx:id="_btnAccept" layoutX="82.0" layoutY="339.0" mnemonicParsing="false" onAction="#onAcceptClicked" text="Accept">
         <tooltip>
            <Tooltip text="Click to Accept" />
         </tooltip></Button>
      <Button fx:id="_btnReject" layoutX="172.0" layoutY="339.0" mnemonicParsing="false" onAction="#onRejectClicked" text="Reject">
         <tooltip>
            <Tooltip text="Click to Reject" />
         </tooltip></Button>
      <TextArea fx:id="_txtOutput" editable="false" layoutX="82.0" layoutY="195.0" prefHeight="132.0" prefWidth="236.0" />
      <Button fx:id="_btnClear" layoutX="259.0" layoutY="339.0" mnemonicParsing="false" onAction="#onClearClicked" text="Clear" />
      <TextField fx:id="_txtInput" layoutX="82.0" layoutY="90.0" prefHeight="27.0" prefWidth="146.0" promptText="Enter a number" />
      <Button fx:id="_btnSubmit" layoutX="255.0" layoutY="90.0" mnemonicParsing="false" onAction="#onSubmitClicked" text="Submit" />
      <Label fx:id="_lblError" layoutX="82.0" layoutY="134.0" prefHeight="17.0" prefWidth="236.0" textFill="#f50808" />
   </children>
</AnchorPane>

      <TextArea fx:id="_txtCourse" editable="false" layoutX="554.0" layoutY="104.0" prefHeight="159.0" prefWidth="232.0" />
      <TextArea fx:id="_txtOutput" editable="false" layoutX="10.0" layoutY="315.0" prefHeight="75.0" prefWidth="779.0" />
   </children>

<AnchorPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="application.MainController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Name:" />
      <Label layoutX="14.0" layoutY="53.0" text="Address:" />

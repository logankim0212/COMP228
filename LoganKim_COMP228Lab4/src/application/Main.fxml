      <Label layoutX="14.0" layoutY="239.0" text="Email:" />
      <TextField fx:id="_txtName" layoutX="170.0" layoutY="9.0" prefHeight="27.0" prefWidth="219.0" />
      <TextField fx:id="_txtAddress" layoutX="170.0" layoutY="48.0" prefHeight="27.0" prefWidth="219.0" />
      <TextField fx:id="_txtProvince" layoutX="170.0" layoutY="83.0" prefHeight="27.0" prefWidth="219.0" />
      <TextField fx:id="_txtCity" layoutX="170.0" layoutY="122.0" prefHeight="27.0" prefWidth="219.0" />
      <TextField fx:id="_txtPostalCode" layoutX="170.0" layoutY="158.0" prefHeight="27.0" prefWidth="219.0" />
      <TextField fx:id="_txtPhoneNumber" layoutX="170.0" layoutY="195.0" prefHeight="27.0" prefWidth="219.0" />
      <TextField fx:id="_txtEmail" layoutX="170.0" layoutY="234.0" prefHeight="27.0" prefWidth="219.0" />

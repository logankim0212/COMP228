      <RadioButton fx:id="_btnCS" layoutX="560.0" layoutY="14.0" mnemonicParsing="false" onAction="#onSelectCS" text="Computer Science">
         <toggleGroup>
            <ToggleGroup fx:id="CourseGrp" />
